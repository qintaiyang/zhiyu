<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:id="@android:id/background" android:height="2.0dip">
        <shape>
            <corners android:radius="2.0dip" />
            <solid android:color="#33004ab7" />
        </shape>
    </item>
    <item android:gravity="center" android:id="@android:id/progress" android:height="2.0dip">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <solid android:color="@color/text_blue" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/text_blue" />
            </shape>
        </clip>
    </item>
</layer-list>
