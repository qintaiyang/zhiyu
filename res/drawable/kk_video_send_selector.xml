<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <corners android:radius="45.0dip" />
            <solid android:color="@color/bule_pressed_color" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <corners android:radius="45.0dip" />
            <solid android:color="@color/text_blue" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_pressed="false">
        <shape>
            <corners android:radius="45.0dip" />
            <solid android:color="@color/text_gray5" />
        </shape>
    </item>
</selector>
